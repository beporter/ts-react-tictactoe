{
  "name": "ts-react-tictactoe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --content-base public/ --hot --color",
    "build": "webpack --mode production",
    "test": "jest || [ \"$NODE_QUIET\" ]",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' 'spec/**/*.{ts,tsx}' || [ \"$NODE_QUIET\" ]",
    "ci": "npm run lint && npm run test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/spec/setupTests.tsx"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testMatch": [
      "<rootDir>/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "enzyme",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project is a part of my [8th Light](https://8thlight.com/) apprenticeship skill-broadening process.",
  "main": "App.tsx",
  "devDependencies": {
    "@types/enzyme": "^3.9.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beporter/ts-react-tictactoe.git"
  },
  "keywords": [
    "typescript",
    "react",
    "tictactoe"
  ],
  "author": "Brian Porter <beporter@users.sourceforge.net>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/beporter/ts-react-tictactoe/issues"
  },
  "homepage": "https://github.com/beporter/ts-react-tictactoe#readme"
}
